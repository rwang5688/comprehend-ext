{
	"name": "Notify!",
	"description": "A Google Chrome extension!",
	"version": "1.0",
	"manifest_version": 3,
    "host_permissions": [
		"https://z5a6imy452.execute-api.us-west-2.amazonaws.com/*",
        "https://postman-echo.com/*"
    ],
	"permissions": [
		"contextMenus",
		"notifications",
		"storage"
	],
	"background": {
	  "service_worker": "background-postman-echo.js"
	},
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": {
		"48": "/assets/icons/48.png",
		"128": "/assets/icons/128.png"
	  }
	},
	"icons": {
		"48": "/assets/icons/48.png",
		"128": "/assets/icons/128.png"
	}
}
